<input id="quiz_input" type="text" placeholder="Enter the name of your quiz" />
<button id="add_quiz_button">Add Quiz</button>

<h2>Quiz List</h2>
<ul id="quiz_list">
    <% @quizzes.each do |quiz| %>
        <li><%= quiz["quiz_name"] %></li>
    <% end %>
</ul>

<button id="show_button">Update Quizzes</button>
