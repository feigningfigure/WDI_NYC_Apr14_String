<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Weapon</th>
      <th>Victory count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gladiators.each do |gladiator| %>
      <tr>
        <td><%= gladiator.name %></td>
        <td><%= gladiator.weapon %></td>
        <td><%= gladiator.victory_count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
