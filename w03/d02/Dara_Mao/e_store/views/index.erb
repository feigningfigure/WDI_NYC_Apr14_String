<ul class="items">
  <% @items.each do |item| %>
    <li>
      <h2><a href="/items/<%= item.id %>/view "><%= item.item_name %></a></h2>
      <div>
        <p><%= item.description %></p>
        <p>
          <%= item.price %> on <%= item.created_at %>
        </p>
      </div>
    </li>
  <% end %>
</ul>
