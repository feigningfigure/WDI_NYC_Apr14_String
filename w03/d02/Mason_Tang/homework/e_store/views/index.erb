<ul class="items">
  <% @items.each do |item| %>
    <li>
      <h2><a href="/items/<%= item.id %>/view "><%= item.name %></a></h2>


          <ul>
            <li>Price: $<%= item.price %></li>
            <li>Description: <%= item.description %></li>
            <li>Quantity: <%= item.quantity %></li>
            <li>Date/Time: <%= item.created_at %></li>
          </ul>

    </li>
  <% end %>
</ul>
