<h3>Planets</h3>

  <div class="planet_list">
  <% @planets.each do |planet| %>
    <div class ="each_planet">
    <h4><a href="planets/<%=planet.id%>/show"><%= planet.name%></a></h4>
    <img src="<%= planet.image_url %>">
    <p>Diameter: <%= planet.diameter%>km</p>
    <p>Mass:     <%= planet.mass%></p>
    <p>Life:     <%= planet.life ? "Yes" : "No"%></p>
    </div>
  <%end%>
  </div>
