<h1>New Moon</h1>
<%= form_for :moon, :url => { :action => "create_moon" } do |f| %>
  <%= f.text_field :planet_id, :value => params[:id] %>
  <%= f.label :name %>
  <%= f.text_field :name%>
  <%= f.label :image_url %>
  <%= f.text_field :image_url%>
  <%= f.label :diameter %>
  <%= f.text_field :diameter %>
  <%= f.label :life %>
  <%= f.text_field :life %>
  <%= f.submit %>
<% end %>
