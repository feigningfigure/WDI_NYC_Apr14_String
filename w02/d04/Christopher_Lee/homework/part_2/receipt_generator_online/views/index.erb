<div class="section group">
  <div class="col span_3_of_12">
  </div>
  <div class="col span_6_of_12 main top_section">
    <h1 class="headline">Print your receipt</h1>
  </div>
</div>
<div class="section group">
  <div class="col span_4_of_12">
  </div>
  <div class="col span_4_of_12 main">
    <form action="/print_receipt" method="POST">
      <input type="text" placeholder="Your Name" name="customer_name" />
      <input type="text" placeholder="Product" name="product" />
      <input type="text" placeholder="Cost of Goods and Services" name="cost_of_goods" />
      <input type="submit" value="Print" />
    </form>
  </div>
</div>


