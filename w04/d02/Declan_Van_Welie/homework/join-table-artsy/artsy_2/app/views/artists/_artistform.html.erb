<%= form_for artist_local do |f| %>
  <%= f.text_field :name, {placeholder: "name"} %>
  <%= f.text_field :birth_year, {placeholder: "birth_year"} %>
  <%= f.text_field :movement, {placeholder: "movement"} %>
  <%= f.text_field :nationality, {placeholder: "nationality"} %>
  <%= f.submit %>
<% end %>

