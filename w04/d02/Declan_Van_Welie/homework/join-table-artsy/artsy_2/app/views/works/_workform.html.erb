<%= form_for work_local do |f| %>
  <%= f.text_field :title, { placeholder: "title" } %>
  <%= f.text_field :medium, { placeholder: "medium" } %>
  <%= f.text_field :year_finished, { placeholder: "year_finished" } %>
    <%= f.text_field :image_url, { placeholder: "image_url" } %>
  <%= f.hidden_field :artist_id, value: artist_id %>
  <%= f.submit %>
<% end %>
