<%= form_for animal_local do |f| %>
<%= f.text_field :name, {placeholder: "name"} %>
<%= f.text_field :scientific_name, {placeholder: "scientific name"} %>
<%= f.text_field :characteristics, {placeholder: "characteristics"} %>
<%= f.text_field :vertebrae_id, {placeholder: "ID of group"} %>
<%= f.submit %>
<% end %>