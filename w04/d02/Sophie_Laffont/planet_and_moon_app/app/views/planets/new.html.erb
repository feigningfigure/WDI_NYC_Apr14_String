<%# this DOES need a crying clown hat %>
<%= form_for @planet, url: "/planets" do |f| %>

<a href="/new">See New Planet</a>
  <ul class="form_elements">
    <li>Name: <%= f.text_field :name, placeholder:"type the planet name" %></li>

   <li>Image_url: <%= f.text_field :image_url, placeholder:"type the image url" %></li>

    <li>Diameter: <%= f.text_field :diameter, placeholder:"type the/diameter" %></li>

    <li>Mass: <%= f.text_field :mass, placeholder:"type the mass" %></li>

    <li>Life: <%= f.text_field :life, placeholder:"true or false" %></li>
  </ul>
  <%= f.submit "Create" %>

<% end %>
