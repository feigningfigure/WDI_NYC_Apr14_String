<%= form_for @player, url: "/players/#{@player.id}" do |f| %>
  <%= f.text_field :name, {placeholder: "name"} %>
  <%= f.text_field :height, {placeholder: "height"} %>
  <%= f.text_field :college, {placeholder: "college"} %>
  <%= f.hidden_field :team_id, :value => @player.team_id %>
  <%= f.submit %>
<% end %>
