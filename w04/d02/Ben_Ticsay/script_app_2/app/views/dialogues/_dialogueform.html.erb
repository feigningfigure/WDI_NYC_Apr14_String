<%= form_for dialogue_local do |f| %>
  <%= f.text_field :paranthetical, { placeholder: "paranthetical" } %>
  <%= f.text_field :content, { placeholder: "content" } %>
  <%= f.hidden_field :scene_id, value: scene_id %>
  <%= f.hidden_field :character_id, value: character_id %>

  <%= f.submit %>
<% end %>
